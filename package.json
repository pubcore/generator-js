{
  "name": "@pubcore/generator-js",
  "version": "1.2.1",
  "description": "Yeomnan generator to create scaffolding for js package",
  "files": [
    "app"
  ],
  "scripts": {
    "test": "mocha --recursive"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown",
    "**/*.js": "eslint . --fix"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:pubcore/generator-js.git"
  },
  "keywords": [
    "yeoman-generator"
  ],
  "author": "pubcore",
  "license": "MIT",
  "engines": {
    "node": ">=10.15.0"
  },
  "devDependencies": {
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^7.2.0",
    "husky": "^5.0.9",
    "mocha": "^8.3.0",
    "nyc": "^15.1.0",
    "prettier": "^2.2.1",
    "yeoman-assert": "^3.1.1",
    "yeoman-test": "^4.0.0"
  },
  "dependencies": {
    "yeoman-generator": "^4.11.0"
  }
}
